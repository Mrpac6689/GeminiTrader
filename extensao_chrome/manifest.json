{
  "manifest_version": 3,
  "name": "Gemini Trader AI",
  "version": "2.0",
  "description": "Analisa o mercado de financeiro e sugere ações de compra e venda em operações intraday.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "notifications",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 